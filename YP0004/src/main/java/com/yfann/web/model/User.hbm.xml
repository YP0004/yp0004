<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yfann.web.model.User" table="t_sys_user">
        <id name="id" column="ID"/>
        <property name="userId" column="USER_ID"/>
        <property name="userName" column="USER_NAME"/>
        <property name="departId" column="DEPART_ID"/>
        <property name="password" column="PASSWORD"/>
        <property name="birthday" column="BIRTHDAY"/>
        <property name="sex" column="SEX"/>
        <property name="idcardNumber" column="IDCARD_NUMBER"/>
        <property name="education" column="EDUCATION"/>
        <property name="foreignCountrie" column="FOREIGN_COUNTRIE"/>
        <property name="graduateSchool" column="GRADUATE_SCHOOL"/>
        <property name="phoneNumber" column="PHONE_NUMBER"/>
        <property name="ip" column="IP"/>
        <property name="countrie" column="COUNTRIE"/>
        <property name="provinceAndCity" column="PROVINCE_AND_CITY"/>
        <property name="address" column="ADDRESS"/>
        <property name="zipCode" column="ZIP_CODE"/>
        <property name="email" column="EMAIL"/>
        <property name="qq" column="QQ"/>
        <property name="headImg" column="HEAD_IMG"/>
        <property name="status" column="STATUS"/>
    </class>
</hibernate-mapping>